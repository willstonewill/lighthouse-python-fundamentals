    "def repeatNumbers(data):\n",
    "    # Put your solution here\n",
    "    result = []\n",
    "    for pair in data:\n",
    "        result.append(str(pair[0])*pair[1])\n",
    "    return ','.join(result)\n",
    "\n",
    "print(repeatNumbers([[1, 10]]))\n",
    "print(repeatNumbers([[1, 2], [2, 3]]))\n",
    "print(repeatNumbers([[10, 4], [34, 6], [92, 2]]))"
   ]
  }
